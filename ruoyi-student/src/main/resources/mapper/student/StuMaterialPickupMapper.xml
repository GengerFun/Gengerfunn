<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.student.mapper.StuMaterialPickupMapper">

    <resultMap type="StuMaterialPickup" id="StuMaterialPickupResult">
        <result property="id"    column="id"    />
        <result property="stuId"    column="stu_id"    />
        <result property="dormitoryId"    column="dormitory_id"    />
        <result property="quantity"    column="quantity"    />
        <result property="materialId"    column="material_id"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectStuMaterialPickupVo">
        select id, stu_id, dormitory_id, quantity, material_id, status, del_flag, create_by, create_time, update_by, update_time, remark from stu_material_pickup
    </sql>

    <select id="selectStuMaterialPickupList" parameterType="StuMaterialPickup" resultMap="StuMaterialPickupResult">
        <include refid="selectStuMaterialPickupVo"/>
        <where>
            <if test="stuId != null "> and stu_id = #{stuId}</if>
            <if test="dormitoryId != null "> and dormitory_id = #{dormitoryId}</if>
            <if test="quantity != null "> and quantity = #{quantity}</if>
            <if test="materialId != null "> and material_id = #{materialId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>

    <select id="selectStuMaterialPickupById" parameterType="Long" resultMap="StuMaterialPickupResult">
        <include refid="selectStuMaterialPickupVo"/>
        where id = #{id}
    </select>

    <insert id="insertStuMaterialPickup" parameterType="StuMaterialPickup" useGeneratedKeys="true" keyProperty="id">
        insert into stu_material_pickup
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="stuId != null">stu_id,</if>
            <if test="dormitoryId != null">dormitory_id,</if>
            <if test="quantity != null">quantity,</if>
            <if test="materialId != null">material_id,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="stuId != null">#{stuId},</if>
            <if test="dormitoryId != null">#{dormitoryId},</if>
            <if test="quantity != null">#{quantity},</if>
            <if test="materialId != null">#{materialId},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateStuMaterialPickup" parameterType="StuMaterialPickup">
        update stu_material_pickup
        <trim prefix="SET" suffixOverrides=",">
            <if test="stuId != null">stu_id = #{stuId},</if>
            <if test="dormitoryId != null">dormitory_id = #{dormitoryId},</if>
            <if test="quantity != null">quantity = #{quantity},</if>
            <if test="materialId != null">material_id = #{materialId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteStuMaterialPickupById" parameterType="Long">
        delete from stu_material_pickup where id = #{id}
    </delete>

    <delete id="deleteStuMaterialPickupByIds" parameterType="String">
        delete from stu_material_pickup where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
